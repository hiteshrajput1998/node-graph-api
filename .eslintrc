{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "rules": {
        "indent": ["warn", 4],
        "quotes": [
            "error",
            "single"
        ],
        "semi": "warn",
        "linebreak-style": 0,
        "no-unused-vars": "warn"
    }
}